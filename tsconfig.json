{
    "compilerOptions": {
        "module": "commonjs",
        "esModuleInterop": true,
        "target": "es2015",
        "noImplicitAny": true,
        "moduleResolution": "node",
        "sourceMap": false,
        "outDir": "dist",
        "baseUrl": "./",
        "paths": {
            "*": [
                "node_modules/*"
            ]
        }
    },  
    "paths":{
        "@interfaces/*":["src/interfaces/*"]
    },
    "include": [
        "src/**/*"
    ],
}